olor'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color')\"></div>\n    <div mbr-class=\"{'container':!fullWidth, 'container-fluid': fullWidth}\">\n        <amp-selector role=\"tablist\" layout=\"container\" class=\"ampTabContainer mbr-flex mbr-jc-c\">\n            <div role=\"tab\" class=\"tabButton\" selected=\"\" option=\"a\">\n              <div class=\"wrapper-button\">\n                  <span class=\"number\">1</span>\n              </div>\n            </div>\n            <div role=\"tabpanel\" class=\"tabContent mbr-flex mbr-column mbr-jc-c align-center\">\n                <div>\n                 <h3 class=\"mbr-section-subtitle content__subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"titleSubtext\" data-app-selector=\".mbr-section-subtitle\">Step 1</h3>\n                <h2 class=\"mbr-section-title content__title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"titleText\" data-app-selector=\".content__title\">\n                    Morning sunshine\n                </h2>\n                <p class=\"mbr-text mbr-fonts-style mbr-pt-4 mbr-pb-2\" mbr-theme-style=\"display-7\" mbr-if=\"showText1\" data-app-selector=\".tabContent .mbr-text\">Lorem ipsum dolor sit amet, consectetur adipisicing <br> elit, sed do eiusmod tempor incididunt ut labore <br> et dolore magna aliqua.</p>\n            </div>\n            <amp-img src=\"@PROJECT_PATH@/assets/images/man1.jpg\" layout=\"responsive\" width=\"1207.111111111111\" height=\"679\" alt=\"img\" class=\"mobirise-loader\">\n                <div placeholder=\"\" class=\"placeholder\"></div>\n                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/man1.jpg\" alt=\"img\">\n            </amp-img>\n        </div>\n            <div role=\"tab\" class=\"tabButton\" option=\"b\" mbr-if=\"tabCount > 1\" mbr-class=\"{'last-child': tabCount == 2}\">\n                <div clas